{
    "store.custom#trocas-e-devolucoes": {
        "children": [
            "flex-layout.row#trocas-devolucoes"
        ]
    },
    "flex-layout.row#trocas-devolucoes": {
        "props": {
            "blockClass": "trocas-devolucoes"
        },
        "children": [
            "flex-layout.col#trocas-devolucoes"
        ]
    },
    "disclosure-state-indicator#trocas-devolucoes": {
        "props": {
            "Show": "icon-caret#down-trocas-devolucoes",
            "Hide": "icon-caret#up-trocas-devolucoes"
        }
    },
    "icon-caret#down-trocas-devolucoes": {
        "props": {
            "orientation": "down"
        }
    },
    "icon-caret#up-trocas-devolucoes": {
        "props": {
            "orientation": "up"
        }
    },
    "flex-layout.col#trocas-devolucoes": {
        "children": [
            "rich-text#trocas-devolucoes",
            "disclosure-layout-group#group-pergunta1",
            "disclosure-layout-group#group-pergunta2",
            "disclosure-layout-group#group-pergunta3",
            "disclosure-layout-group#group-pergunta4",
            "disclosure-layout-group#group-pergunta5"
        ]
    },
    "rich-text#trocas-devolucoes": {
        "props": {
            "text": "## Perguntas Frequentes"
        }
    },
    "disclosure-layout-group#group-pergunta1": {
        "children": [
            "disclosure-layout#questao1"
        ]
    },
    "disclosure-layout#questao1": {
        "props": {
            "animated": true
        },
        "children": [
            "disclosure-trigger##questao1",
            "disclosure-content##questao1"
        ]
    },
    "disclosure-trigger##questao1": {
        "props": {
            "blockClass": "question",
            "as": "div"
        },
        "children": [
            "disclosure-state-indicator#trocas-devolucoes",
            "rich-text##questao1-pergunta"
        ]
    },
    "disclosure-content##questao1": {
        "props": {
            "blockClass": "answer"
        },
        "children": [
            "rich-text##questao1-resposta"
        ]
    },
    "rich-text##questao1-pergunta": {
        "props": {
            "text": "OS PRODUTOS SÃO ORIGINAIS?"
        }
    },
    "rich-text##questao1-resposta": {
        "props": {
            "text": "Sim, todos os produtos vendidos em nossa loja são 100% originais e adquiridos diretamente dos fornecedores credenciados pelas marcas. \n  Além disso, acompanham nota fiscal."
        }
    },
